<%- include("header")-%>
<body>
    <div class="box" id="heading">
    <h1><%= day %></h1>
    </div>
    
    <div class="box" >

        <% for(var i=0;i<newlistItems.length;i++){ %>
            <div class="item">
            <input type="checkbox">
            <p><%= newlistItems[i] %></p>
            </div>
        <% } %>
    
    <form class="" action="/" method="POST">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="button" value=<%=day%> >+</button>
    </form>
    </div>
</body>
</html>